<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<managed-bean>
		<managed-bean-name>itemAcervo</managed-bean-name>
		<managed-bean-class>museu.controller.acervo.ItemDoAcervo</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ajude</managed-bean-name>
		<managed-bean-class>museu.controller.AjudeOMuseu</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>dPage</managed-bean-name>
		<managed-bean-class>museu.controller.DPage</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>news</managed-bean-name>
		<managed-bean-class>museu.controller.News</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>events</managed-bean-name>
		<managed-bean-class>museu.controller.Events</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>notice</managed-bean-name>
		<managed-bean-class>museu.controller.New</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>eventP</managed-bean-name>
		<managed-bean-class>museu.controller.Event</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>index</managed-bean-name>
		<managed-bean-class>museu.controller.Index</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>indexAcervo</managed-bean-name>
		<managed-bean-class>museu.controller.acervo.AcervoIndex</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>museu.controller.adm.Login</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>indexAdm</managed-bean-name>
		<managed-bean-class>museu.controller.adm.Index</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>acessosPortal</managed-bean-name>
		<managed-bean-class>museu.controller.adm.AcessosAoPortal</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>busca</managed-bean-name>
		<managed-bean-class>museu.controller.Busca</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>primeiroAcesso</managed-bean-name>
		<managed-bean-class>museu.controller.adm.PrimeiroAcesso</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>viewBusca</managed-bean-name>
		<managed-bean-class>museu.controller.ViewBusca</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Portal</display-name>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>acervo</from-outcome>
			<to-view-id>/pages/acervo/acervo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listagem</from-outcome>
			<to-view-id>/pages/acervo/listagem.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>fauna</from-outcome>
			<to-view-id>/pages/acervo/acervo_fauna.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>flora</from-outcome>
			<to-view-id>/pages/acervo/acervo_flora.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>item</from-outcome>
			<to-view-id>/pages/acervo/item_do_acervo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>pagina</from-outcome>
			<to-view-id>/pages/pagina.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>events</from-outcome>
			<to-view-id>/pages/events.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>news</from-outcome>
			<to-view-id>/pages/news.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/pages/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>event</from-outcome>
			<to-view-id>/pages/event.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>/pages/new.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ajudeOMuseu</from-outcome>
			<to-view-id>/pages/ajudeOMuseu.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<application>
		<resource-bundle>
			<base-name>museu.properties.location</base-name>
			<var>strings</var>
		</resource-bundle>
		<locale-config>
			<default-locale>pt</default-locale>
			<supported-locale>en</supported-locale>
		</locale-config>
	</application>
</faces-config>
